{
  "name": "Custom Conditions Module",
  "remarks": "Assigns conditions based on age, gender, and grouping",
  "states": {
    "Initial": {
      "type": "Initial",
       "direct_transition": "Age_Gender_Split"
    },
    "Age_Gender_Split": {
      "type": "Guard",
      "conditions": [
        {
          "condition_type": "And",
          "conditions": [
            {
              "condition_type": "Gender",
              "gender": "F"
            },
            {
              "condition_type": "Age",
              "operator": ">=",
              "value": 18
            }
          ]
        }
      ],
      "allow": "Assign_Grouped_Conditions",
      "otherwise": "Assign_Ungrouped_Conditions"
    },
    "Assign_Grouped_Conditions": {
      "type": "SetAttribute",
      "attribute": "condition_group",
      "value": "grouped",
      "direct_transition": "Grouped_Condition_Assignment"
    },
    "Assign_Ungrouped_Conditions": {
      "type": "SetAttribute",
      "attribute": "condition_group",
      "value": "ungrouped",
      "direct_transition": "Ungrouped_Condition_Assignment"
    },
    "Grouped_Condition_Assignment": {
      "type": "SetAttribute",
      "attribute": "group_assignment_started",
      "value": true,
      "direct_transition": "End"
    },
    "Ungrouped_Condition_Assignment": {
      "type": "SetAttribute",
      "attribute": "ungroup_assignment_started",
      "value": true,
      "direct_transition": "End"
    },
    "End": {
      "type": "Terminal"
    },

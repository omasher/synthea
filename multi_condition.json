        "Entry point for patients in Massachusetts"
      ]
    },
    "Texas": {
      "type": "Site",
      "direct_transition": "Age_Branching",
      "remarks": [
        "Entry point for patients in Texas"
      ]
    },
    "Age_Branching": {
      "type": "DistributedTransition",
      "distributed_transition": [
        {
          "condition": {
            "type": "Age",
            "operator": "<=",
            "value": 10,
            "unit": "years"
          },
          "transition": "Age_0_10",
          "distribution": 0.35
        },
        {
          "condition": {
            "type": "And",
            "conditions": [
              {
                "type": "Age",
                "operator": ">",
                "value": 10
              },
              {
                "type": "Age",
                "operator": "<=",
                "value": 20
              }
            ]
          },
          "transition": "Age_11_20",
          "distribution": 0.3
        },
        {
          "condition": {
            "type": "And",
            "conditions": [
              {
                "type": "Age",
                "operator": ">",
                "value": 20
              },
              {
                "type": "Age",
                "operator": "<=",
                "value": 40
              }
            ]
          },
          "transition": "Age_21_40",
          "distribution": 0.15
        },
        {
          "condition": {
            "type": "And",
            "conditions": [
              {
                "type": "Age",
                "operator": ">",
                "value": 40
              },
              {
                "type": "Age",
                "operator": "<=",
                "value": 65
              }
            ]
          },
          "transition": "Age_41_66",
          "distribution": 0.17
        },
        {
          "condition": {
            "type": "Age",
            "operator": ">",
            "value": 65
          },
          "transition": "Age_66_plus",
          "distribution": 0.03
        }
      ]
    },
